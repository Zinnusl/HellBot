{
	"rootDirs": ["commands", "core"],
	"compilerOptions": {
		"noImplicitAny": true,
		"strictNullChecks": true,
		"moduleResolution": "node",
		"target": "ES2021",
		"module": "commonjs",
		"outDir": "dist",
		"sourceMap": true,
		"baseUrl": ".",
		"paths": {
			"#commands/*": ["commands/*"],
			"#core/*": ["core/*"]
		}
	}
}

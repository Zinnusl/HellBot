{
	"rootDirs": ["core", "extensions"],
	"compilerOptions": {
		"noImplicitAny": true,
		"strictNullChecks": true,
		"moduleResolution": "node",
		"target": "ES2021",
		"module": "commonjs",
		"outDir": "dist",
		"sourceMap": true,
		"baseUrl": ".",
		"paths": {
			"#core/*": ["core/*"],
			"#extensions/*": ["extensions/*"],
		}
	}
}
